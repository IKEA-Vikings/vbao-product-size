{
  "name": "vbao-product-size",
  "version": "",
  "description": "",
  "author": "",
  "license": "",
  "engines": {
    "node": ">=6.13.0"
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "@shelf/jest-mongodb",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "**/server/*.{js,jsx}",
      "**/server/database/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/server/server.js",
      "!**/server/database/seed.js"
    ],
    "globals": {
      "window": {}
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "scripts": {
    "seed": "node ./server/database/seed.js",
    "test": "jest --no-cache --collect-coverage",
    "start": "nodemon ./server/server.js",
    "build": "webpack --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@shelf/jest-mongodb": "^1.2.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.3",
    "enzyme": "^3.11.0",
    "eslint-config-hackreactor": "github:reactorcore/eslint-config-hackreactor",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "style-loader": "^2.0.0",
    "supertest": "^6.1.3",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.11.19",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
